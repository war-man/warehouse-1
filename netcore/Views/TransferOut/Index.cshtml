@model IEnumerable<netcore.Models.Invent.TransferOut>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminlteNetcoreIndex.cshtml";
    ViewData["EntityIcon"] = "fa fa-upload";
    ViewData["ListTitle"] = "Danh sách xuất điều chuyển hàng hóa";
    ViewData["IndexInfo"] = "Xuất điều chuyển  hàng hóa là một quá trình xuất (giao) hàng hóa từ kho dựa trên đơn hàng chuyển nhượng (TO).";
}


<table class="table table-hover table-striped" style="width:100%">
    <thead>
        <tr>
            <th></th>
            <th>
                @Html.DisplayNameFor(model => model.transferOutNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.transferOutDate)
            </th>
            <th class="hidden">
                @Html.DisplayNameFor(model => model.description)
            </th>
            <th class="hidden">
                @Html.DisplayNameFor(model => model.branchIdFrom)
            </th>
            <th class="hidden">
                @Html.DisplayNameFor(model => model.warehouseIdFrom)
            </th>
            <th class="hidden">
                @Html.DisplayNameFor(model => model.branchIdTo)
            </th>
            <th class="hidden">
                @Html.DisplayNameFor(model => model.warehouseIdTo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.transferOrder)
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                <div class="tools">
                    <a asp-action="Details" asp-route-id="@item.transferOutId" class="btn btn-primary btn-xs"><i class="fa fa-list"></i></a>
                    <a asp-action="Delete" asp-route-id="@item.transferOutId" class="btn btn-danger btn-xs"><i class="fa fa-trash"></i></a>
                </div>
          
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.transferOutNumber)
            </td>
            <td>
                @item.transferOutDate.Date.ToString("dd-MMM-yyyy")
            </td>
            <td class="hidden">
                @Html.DisplayFor(modelItem => item.description)
            </td>
            <td class="hidden">
                @Html.DisplayFor(modelItem => item.branchIdFrom)
            </td>
            <td class="hidden">
                @Html.DisplayFor(modelItem => item.warehouseIdFrom)
            </td>
            <td class="hidden">
                @Html.DisplayFor(modelItem => item.branchIdTo)
            </td>
            <td class="hidden">
                @Html.DisplayFor(modelItem => item.warehouseIdTo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.transferOrder.transferOrderNumber)
            </td>

        </tr>
        }
    </tbody>
</table>
