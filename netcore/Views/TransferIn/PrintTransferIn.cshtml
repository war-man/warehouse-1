@model netcore.Models.Invent.TransferIn
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PrintTransferIn</title>
</head>
<body onload="window.print();">
    <div class="wrapper">
        <!-- Main content -->
        <section class="invoice">
            <!-- title row -->
            <div class="row">
                <div class="col-xs-12">
                    <h2 class="page-header">
                        <i class="fa fa-globe"></i> Phiếu nhập điều chuyển kho
                        <small class="pull-right">@Model.transferInNumber</small>
                    </h2>
                </div>
                <!-- /.col -->
            </div>
            <!-- info row -->
            <div class="row invoice-info">
                <div class="col-sm-4 invoice-col">
                    Từ kho
                    <address>
                        <strong>@Model.warehouseFrom.warehouseName</strong><br>
                        @Model.warehouseFrom.street1<br>
                        @Model.warehouseFrom.street2<br>
                        @Model.warehouseFrom.city<br>
                        Người lập phiếu: @Model.transferOrder.picName
                    </address>
                </div>
                <!-- /.col -->
                <div class="col-sm-4 invoice-col">
                    Đến kho
                    <address>
                        <strong>@Model.warehouseTo.warehouseName</strong><br>
                        @Model.warehouseTo.street1<br>
                        @Model.warehouseTo.street2<br>
                        @Model.warehouseTo.city<br>
                        Người lập phiếu: @Model.transferOrder.picName
                    </address>
                </div>
                <!-- /.col -->
                <div class="col-sm-4 invoice-col">
                    <b>Số lô hàng (TI):</b> @Model.transferInNumber<br>
                    <b>Ngày lô hàng (TI):</b> @Model.transferInDate.ToString("dd-MMM-yyyy")<br>
                    <b>Số (TO):</b> @Model.transferOrder.transferOrderNumber<br>
                    <b>Ngày (TO):</b> @Model.transferOrder.transferOrderDate.ToString("dd-MMM-yyyy")<br>
                    <b>Người lập phiếu:</b> @Model.transferOrder.picName<br>

                </div>
                <!-- /.col -->
            </div>
            <!-- /.row -->
            <!-- Table row -->
            <div class="row">
                <div class="col-xs-12 table-responsive">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>Số lượng</th>
                                <th>Hàng hóa</th>
                                <th>Mã hàng hóa</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.transferInLine)
                            {
                                <tr>
                                    <td>@item.qty</td>
                                    <td>@item.product.productCode</td>
                                    <td>@item.product.productName</td>
                                </tr>
                            }

                        </tbody>
                    </table>
                </div>
                <!-- /.col -->
            </div>
            <!-- /.row -->
            <div class="row">
                <!-- accepted payments column -->
                <div class="col-xs-6">
                    <p class="lead">Thông tin mô tả:</p>
                    <p class="text-muted well well-sm no-shadow" style="margin-top: 10px;">
                        @Model.description
                    </p>
                </div>
                <!-- /.col -->
                <div class="col-xs-6">

                </div>
                <!-- /.col -->
            </div>
            <!-- /.row -->
        </section>
        <!-- /.content -->
    </div>
    <!-- ./wrapper -->
    @Html.Partial("_AdminlteNetcoreBasicStyles")
    @Html.Partial("_AdminlteNetcoreBasicScripts")
</body>
</html>
